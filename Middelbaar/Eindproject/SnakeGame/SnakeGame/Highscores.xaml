<Window x:Class="SnakeGame.Highscores"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SnakeGame"
        mc:Ignorable="d"
        Title="Highscores" Height="400" Width="500" Background="Black">
    <Grid>
        <ListBox x:Name="lstHighscores" HorizontalAlignment="Left" Height="210" Margin="105,91,0,0" VerticalAlignment="Top" Width="291"/>
        <Button x:Name="btnExit" Content="Sluit af" HorizontalAlignment="Left" Margin="338,316,0,0" VerticalAlignment="Top" Width="98" Height="30" Click="BtnExit_Click"/>
        <Button x:Name="btnOpnieuw" Content="Speel opnieuw" HorizontalAlignment="Left" Margin="75,316,0,0" VerticalAlignment="Top" Width="100" Height="31" Click="BtnOpnieuw_Click"/>
        <Label x:Name="lblHighscore" Content="Highscores" HorizontalAlignment="Left" Margin="114,23,0,0" VerticalAlignment="Top" FontFamily="MS Reference Sans Serif" FontSize="48" Foreground="White"/>
        <Button x:Name="btnExit_Copy" Content="Sluit af" HorizontalAlignment="Left" Margin="338,316,0,0" VerticalAlignment="Top" Width="98" Height="30" Click="BtnExit_Click"/>

    </Grid>
</Window>
