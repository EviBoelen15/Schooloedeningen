        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Label" HorizontalAlignment="Left" Margin="117,95,0,0" VerticalAlignment="Top" Width="108"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="117,146,0,0" VerticalAlignment="Top" Width="108"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="117,310,0,0" VerticalAlignment="Top" Width="108"/>
