        <Button x:Name="btnWissen" Content="Wissen" HorizontalAlignment="Left" Margin="618,284,0,0" VerticalAlignment="Top" Width="120" Height="41" Click="btnWissen_Click"/>
        <Button x:Name="btnAfsluiten" Content="Afsluiten" HorizontalAlignment="Left" Margin="618,345,0,0" VerticalAlignment="Top" Width="120" Height="41" Click="btnAfsluiten_Click"/>

    </Grid>
</Window>
