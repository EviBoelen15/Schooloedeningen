        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Oefening23"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Land" HorizontalAlignment="Left" Margin="245,68,0,0" VerticalAlignment="Top" Width="128"/>
        <Label Content="Bevolking" HorizontalAlignment="Left" Margin="245,118,0,0" VerticalAlignment="Top" Width="128"/>
        <Label Content="Groei" HorizontalAlignment="Left" Margin="245,168,0,0" VerticalAlignment="Top" Width="128"/>
        <TextBox x:Name="txtLand" HorizontalAlignment="Left" Height="26" Margin="389,68,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="167"/>
