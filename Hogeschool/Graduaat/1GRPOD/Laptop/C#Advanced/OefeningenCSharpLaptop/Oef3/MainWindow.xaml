        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Oef3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtResultaat" HorizontalAlignment="Left" Height="207" Margin="95,45,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="611" FontFamily="Consolas"/>
        <Button x:Name="btnInlezen" Content="Inlezen" HorizontalAlignment="Left" Margin="95,286,0,0" VerticalAlignment="Top" Width="101" Height="44" Click="btnInlezen_Click"/>
        <Button x:Name="btnVerwerken" IsEnabled="false" Content="Verwerken" HorizontalAlignment="Left" Margin="264,286,0,0" VerticalAlignment="Top" Width="101" Height="44" Click="btnVerwerken_Click"/>
        <Button x:Name="btnNalezen" Content="Nalezen" HorizontalAlignment="Left" Margin="432,286,0,0" VerticalAlignment="Top" Width="101" Height="44" Click="btnNalezen_Click"/>
        <Button x:Name="btnSluiten" Content="Sluiten" HorizontalAlignment="Left" Margin="605,286,0,0" VerticalAlignment="Top" Width="101" Height="44" Click="btnSluiten_Click"/>

    </Grid>
</Window>
